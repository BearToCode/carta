{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"composite": true,
		"ignoreDeprecations": "5.0",
		"plugins": []
	}
}
